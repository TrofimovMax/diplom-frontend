{
  "name": "diplom-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 8000",
    "prebuild": "npm run graphql",
    "build": "next build",
    "prestart": "npm run graphql",
    "start": "next start -p 8000",
    "codegen": "graphql-codegen --config codegen.ts",
    "remove-codegen-files": "find ./src -name \"__generated__\" -exec rm -rf '{}' +",
    "graphql": "npm run remove-codegen-files && npm run codegen",
    "lint": "next lint"
  },
  "dependencies": {
    "@apollo/client": "3.8.1",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.13",
    "@pbe/react-yandex-maps": "^1.2.4",
    "axios": "^1.3.4",
    "eslint": "8.35.0",
    "eslint-config-next": "13.2.1",
    "graphql": "16.7.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "next": "13.2.1",
    "react": "18.2.0",
    "react-cookie": "^4.1.1",
    "react-dom": "18.2.0",
    "react-material-ui-carousel": "^3.4.2",
    "react-query": "^3.39.3",
    "validator": "^13.9.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-codegen/named-operations-object": "^2.3.1",
    "@graphql-codegen/near-operation-file-preset": "^2.5.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "typescript": "^5.2.2"
  }
}
